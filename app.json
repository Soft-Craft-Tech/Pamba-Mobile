{
  "expo": {
    "name": "Pamba",
    "slug": "pamba",
    "version": "0.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pamba",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash@3x.png",
      "resizeMode": "contain",
      "backgroundColor": "#DB1471",
      "mdpi": "./assets/images/splash.png",
      "xhdpi": "./assets/images/splash@2x.png",
      "xxhdpi": "./assets/images/splash@3x.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dclintonn.pamba"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#DB1471"
      },
      "package": "com.dclintonn.pamba",
      "versionCode": 3
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Pamba accesses your photos for your profile"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "291a330a-5693-4520-a94c-0c7fd78ccd86"
      }
    },
    "owner": "martinki"
  }
}
